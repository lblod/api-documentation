{
  "name": "editor-documentation",
  "description": "LBLOD editor Documentation pages",
  "version": "0.0.0",
  "options": {
    "paths": [
      "app",
      "node_modules/ember-rdfa-editor/addon",
      "node_modules/ember-rdfa-editor-citaten-plugin/addon",
      "node_modules/ember-rdfa-editor-console-logger-plugin/addon",
      "node_modules/ember-rdfa-editor-date-plugin/addon",
      "node_modules/ember-rdfa-editor-standard-template-plugin/addon",
      "node_modules/ember-rdfa-editor-mandataris-plugin/addon",
      "node_modules/ember-rdfa-editor-plugin-system-dispatcher/addon",
      "node_modules/ember-rdfa-editor-plugin-system-dispatcher/app",
      "node_modules/ember-contenteditable-editor/addon",
      "node_modules/@lblod/ember-vo-webuniversum/addon"
    ],
    "exclude": "vendor",
    "outdir": "docs",
    "linkNatives": true,
    "quiet": true,
    "parseOnly": false,
    "lint": false,
    "enabledEnvironments": ["development", "production"],
    "themedir": "node_modules/yuidoc-ember-theme",
    "helpers": [
      "node_modules/yuidoc-ember-theme/helpers/helpers.js"
    ]
  }
}
